          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider1\" group=\"Sliders & Galleries\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <input type=\"id\" name=\"sliderId\">\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\" condition=\"!fullScreen\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\" condition=\"!fullScreen\">\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Fade Effect\" name=\"fade\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"3\" max=\"10\" value=\"5\" step=\"1\" condition=\"autoplay\">\n        <header>Slide</header>\n        <input type=\"checkbox\" title=\"Caption\" name=\"showCaption\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <header>Background</header>\n        <input type=\"color\" title=\"Color\" name=\"bgColor\" value=\"#fafafa\" selected>\n    </mbr-parameters>\n    <div id=\"{{sliderId}}\" class=\"carousel slide\" mbr-class=\"{'carousel-fade': fade}\" data-ride=\"{{autoplay ? 'carousel' : false}}\" data-bs-ride=\"{{autoplay ? 'carousel' : false}}\" data-interval=\"{{interval*1000}}\" data-bs-interval=\"{{interval*1000}}\">\n      <div class=\"full-screen\">\n          <ol class=\"carousel-indicators\">\n              <li data-target=\"#{{sliderId}}\" data-bs-target=\"#{{sliderId}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"active\"></li>\n              <li data-target=\"#{{sliderId}}\" data-bs-target=\"#{{sliderId}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\"></li>\n              \n          </ol>\n          <div class=\"carousel-inner\">            \n              <div class=\"carousel-inner\" role=\"listbox\">\n                <div class=\"carousel-item slider-fullscreen-image active\" style=\"background-image: url(@PROJECT_PATH@/assets/images/cover-homepage-summit-2023-en.jpg);\">\n\n                  <div class=\"container container-slide\">\n                    <div class=\"image_wrapper\">                    \n                      <a href=\"https://event.anphabe.com/vietnamexcellence/2023/\"><img src=\"@PROJECT_PATH@/assets/images/cover-homepage-summit-2023-en.jpg\"></a>\n                      <div class=\"carousel-caption justify-content-center\">                        \n                      </div>\n                    </div>\n                  </div>\n                </div>       \n                \n                <div class=\"carousel-item slider-fullscreen-image\" style=\"background-image: url(@PROJECT_PATH@/assets/images/bg-cover5-3.jpg);\">\n                  <div class=\"container container-slide\">\n                    <div class=\"image_wrapper\">                    \n                      <img src=\"@PROJECT_PATH@/assets/images/bg-cover5-3.jpg\">\n                      <div class=\"carousel-caption align-left\">\n                        <div class=\"col-10 align-left intro-style\">\n                          <ul class=\"mbr-text mbr-white mbr-fonts-style display-7\">\n                              <li><span class=\"mbri-success\"></span>Celebrate the Excellence</li>\n                              <li><span class=\"mbri-success\"></span>Inspire for change</li>\n                              <li><span class=\"mbri-success\"></span>Multiply success stories</li>                   \n                          </ul>\n                          <div class=\"mt-3 mbr-section-btn\" buttons=\"0\"><a class=\"btn  border-gradient border-gradient-green btn-primary display-4 buttons-style\" href=\"nominate.html\">NOMINATE NOW</a></div>\n                        </div>\n                      </div>\n         