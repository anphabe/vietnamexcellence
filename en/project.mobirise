" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type!=='color'\">Overlay</header>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#fafafa\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth,\n                    'container-fluid': fullWidth}\">\n        \n        <div class=\"row mbr-gallery\">\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/nashtech.jpg\" alt=\"NASHTECH\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>NASHTECH</b></h4>\n                </div>\n            </div>\n\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/oppo.jpg\" alt=\"OPPO VIETNAM\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>OPPO VIETNAM</b></h4>\n                </div>\n            </div>\n            \n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/grab-logo.jpg\" alt=\"GRAB\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>GRAB</b></h4>\n                </div>\n            </div><div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/astra.jpg\" alt=\"AstraZeneca Vietnam\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>ASTRAZENECA VIETNAM</b></h4>\n                </div>\n            </div>\n            \n        </div>\n\n        <div class=\"modal mbr-slider\" id=\"{{idGallery}}-modal\" tabindex=\"-1\" role=\"dialog\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\" role=\"document\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-body\">\n                        <div id=\"lb-{{idGallery}}\" class=\"carousel slide\" mbr-class=\"{'carousel-fade': fade}\" data-ride=\"{{autoplay ? 'carousel' : false}}\" data-bs-ride=\"{{autoplay ? 'carousel' : false}}\" data-interval=\"{{interval*1000}}\" data-bs-interval=\"{{interval*1000}}\">\n                            <div class=\"carousel-inner\">\n                                <div class=\"carousel-item active\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">Workplace Culture</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>NASHTECH</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/deloite.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">As NashTech ambition is to build a technology company that positively affects people's lives, we aim to develop our workforce from good to great and provide an excellent working environment through 3 key cultural pillars:</p>\n                                               <ul style=\"margin-bottom: 1rem\">\n                                                  <li class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">High performing &amp; rewarding culture</li>\n                                                  <li class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Learning &amp; Sharing culture</li>\n                                                  <li class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Company as the second home</li>\n                                               </ul>\n                                               <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">NashTech believes that the better employee engagement we make, the higher business success we can get.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                                                                \n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">Workplace Culture</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>OPPO VIETNAM</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/mec.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Culture-based organization strategy has led to breakout achievements for OPPO Vietnam despite the difficulties from the beginning. Shaped the company’s culture, increased culture awareness &amp; understanding, practiced by specific activities, OPPO Vietnam has been creating a happy working environment for employees, maintaining a sustainable human resource with happiness indexes increasing years on years. </p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                                \n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">Workplace Culture</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>GRAB</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100 hr-winner\" src=\"false\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">The digitalised development in the new normal era has posed many challenges in building and developing workplace culture. Grab believed that talent is the heart of the company and a success of a company is come from the happy workforce. Therefore, Grab strives to create a wonderful working environment to ensure both mental and physical health for all Grabbers, as well as create an empowerment to optimize Grabbers' potential. Amidst the complicated changes of Covid, Grab believed that engagement activities always play an important role in enhancing the cooperation between functions and among team members.</p>\n\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">At Grab, corporate culture is considered as a DNA of Grabber, together with 4H principles (Hear-Hunger-Honor-Humility) is instilled in individual and every single team and guiding all business and people related decision. Grabbers constantly focus on the community, together achieve Grab's mission which is driving South East Asia forward.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div><div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">Workplace Culture</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>AstraZeneca Vietnam</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100 hr-winner\" src=\"@PROJECT_PATH@/assets/images/astra.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Continuing the success of its 27-year journey in Vietnam, AstraZeneca Vietnam is entering a new chapter of growth with the ambition of leading the industry while making holistic contributions to the country’s health system by boosting investments in research &amp; development, sustainability, and healthcare capacity building. The realisation of these ambitious goals requires a strong drive and aspiration from the AstraZeneca team. </p>\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">With an excellent strategy and execution to strengthen its workplace culture, AstraZeneca Vietnam has not only achieved its impressive growth targets, but also developed steadily in the face of industry challenges stemming from the COVID-19 pandemic. An important element that has played a role in AstraZeneca Vietnam’s successes are its ability to transform and nurture a culture that is entrepreneurial, collaborative, empowering and open to new ideas. This culture has created an ambitious, highly engaged team that is agile and adaptive to stay ahead of the curve. </p>\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">AstraZeneca Vietnam has established its reputation as a great place to work for local talent in the health sector.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n\n                                \n                            </div>\n\n                            <ol class=\"carousel-indicators\">\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"active\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"2\" data-bs-slide-to=\"2\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\"></li>                                \n                            </ol>\n                            <a role=\"button\" href class=\"close\" data-dismiss=\"modal\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\n                            </a>\n                            <a class=\"carousel-control-prev carousel-control\" href=\"#lb-{{idGallery}}\" role=\"button\" data-slide=\"prev\" data-bs-slide=\"prev\">\n                                <span class=\"mobi-mbri mobi-mbri-arrow-prev\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden\">Previous</span>\n                            </a>\n                            <a class=\"carousel-control-next carousel-control\" href=\"#lb-{{idGallery}}\" role=\"button\" data-slide=\"next\" data-bs-slide=\"next\">\n                                <span class=\"mobi-mbri mobi-mbri-arrow-next\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden\">Next</span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
