          "_name": "gallery5",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"gallery5 mbr-gallery\" group=\"Sliders & Galleries\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\">\n    <mbr-parameters>\n        <input type=\"id\" name=\"idGallery\">\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <select title=\"Columns\" name=\"cardsWidth\">\n            <option value=\"12\">1</option>\n            <option value=\"6\">2</option>\n            <option value=\"4\">3</option>\n            <option value=\"3\" selected>4</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <header>Images</header>\n        <input type=\"checkbox\" title=\"Auto Size\" name=\"autoSize\">\n        <input type=\"range\" name=\"imageHeight\" title=\"Height\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"autoSize && showItemSubtitle\">\n        <input type=\"checkbox\" title=\"Spacing\" name=\"spacing\" checked>\n        <input type=\"checkbox\" title=\"Caption\" name=\"showItemSubtitle\" checked>\n        <header>Popup Slider</header>\n        <input type=\"checkbox\" title=\"Fade Effect\" name=\"fade\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"3\" max=\"10\" value=\"5\" step=\"1\" condition=\"autoplay\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type!=='color'\">Overlay</header>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#fafafa\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth,\n                    'container-fluid': fullWidth}\">\n        \n        <div class=\"row mbr-gallery\">\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/cocacola.jpg\" alt=\"Coca-Cola Beverages Vietnam Ltd.\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>&nbsp;COCA-COLA VIETNAM</b></h4>\n                </div>\n            </div>\n\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/nguyenductai.jpg\" alt=\"Mr. NGUYỄN ĐỨC TÀI\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\">Mr. <b>NGUYỄN ĐỨC TÀI</b></h4>\n                      <h6 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-4\" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><i>Co-founder, Chairman - Mobile World Investment Corporation (MWG)</i></h6>\n                </div>\n            </div>\n            \n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/sanofi.jpg\" alt=\"SANOFI AVENTIS VIETNAM\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>SANOFI AVENTIS VIETNAM</b></h4>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/lazada.jpg\" alt=\"LAZADA VIETNAM\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-2 mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>LAZADA VIETNAM</b></h4>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"modal mbr-slider\" id=\"{{idGallery}}-modal\" tabindex=\"-1\" role=\"dialog\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\" role=\"document\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-body\">\n                        <div id=\"lb-{{idGallery}}\" class=\"carousel slide\" mbr-class=\"{'carousel-fade': fade}\" data-ride=\"{{autoplay ? 'carousel' : false}}\" data-bs-ride=\"{{autoplay ? 'carousel' : false}}\" data-interval=\"{{interval*1000}}\" data-bs-interval=\"{{interval*1000}}\">\n                            <div class=\"carousel-inner\">\n                                <div class=\"carousel-item active\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">People Champion</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>Coca-Cola Beverages Vietnam Ltd.</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/cocacola.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Guide by the Purpose of refreshing the world and making a difference, people play a key role in leading Coca-Cola Beverages Vietnam’s growth as an organization that will allow them to deliver even more refreshing moments and effecting greater positive impact to the people, business and communities. There have been many challenges in people management and development that the company have faced which required them to change how they lead, work and learn to emerge stronger and accelerate their growth. The Leadership of Coca-Cola Vietnam has led and inspired a strategic solution: Building Growth Culture for Resilience Organization as they all believe in the revolution of Learning and Development which enables organization success. Coca-Cola Beverages Vietnam has so far had great achievements in maximizing the experience for their people and preparing a stronger networked leaders to embrace high-performing culture to articulate differentiated capabilities to win in the marketplace and affirm their top-of-mind Employer.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                                                                \n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">People Champion</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Mr. <b>NGUYỄN ĐỨC TÀI</b></h1>\n                                            <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Co-founder, Chairman - Mobile World Investment Corporation (MWG)</p>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/nguyenductai.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">During the 17 year-journey of MWG, Mr. Duc Tai has been the captain who devotes to bring all employees happiness, properties, and pride as being MWGers. He has led the company to achieve notable achievements with a clear human management philosophy and commitment to develop resources from employees to managers, right from the very first years.</p>\n\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Mr. Duc Tai and his team have solved each conundrum of human resource development, motivate the whole team, and connect more than 60,000 employees in the organization. He encourages and serves as the role model who aims to build a human-centric working environment that also challenges employees to expand their abilities and skills, while allowing for internal advancement and craft the next generation of leaders.</p>\n\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Despite a difficult year due to the global pandemic, MWG continued to record 6% growth in revenue, reaching more than 108,546 billion Dong, and 2% growth in NPAT with a total of 3,920 billion Dong in 2020, and securing jobs for more than 60,000 employees.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                                \n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">People Champion</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>SANOFI AVENTIS VIETNAM</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/sanofi.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">The Covid-19 pandemic greatly posed challenges to Sanofi's operation and its talent management. In such time of uncertainty, a new way of work and adaptive strategies must be applied to boost efficiency in operation.</p>\n                                               <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">The 4 Generals managers join hands to take immediate actions to tackle the waves of change. In the process, not only the business mission of leading the pharma market is achieved, but relationships within the company, work efficiency, and strategies for market engagement are also enhanced.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n\n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">People Champion</h2>\n                                            <h1 class=\"mbr-section-title mbr-fonts-style m-0 winner-name mt-2\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>LAZADA VIETNAM</b></h1>\n                                        </div>\n                                        <div class=\"col-12 col-md-4\">\n                                            <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/lazada.jpg\" alt>\n                                        </div>\n                                        <div class=\"col-12 col-md-8\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">Founded in 2012, Lazada is the pioneer platform that plays the leading role in developing and shaping e-commerce in Vietnam. Placing people at the core value, the company focus on nurturing talents in Vietnam as the top priority. Therefore, we focus on recruiting the right talents, empower and nurturing talents through capability building, internal engagement, all in a value-based &amp; performance culture. Thanks to its efforts, Lazada Vietnam team are proud to claim as talent incubator for the industry.</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n\n                            <ol class=\"carousel-indicators\">\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"active\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"2\" data-bs-slide-to=\"2\"></li><li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\"></li>\n                                \n                            </ol>\n                            <a role=\"button\" href class=\"close\" data-dismiss=\"modal\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\n                            </a>\n                            <a class=\"carousel-control-prev carousel-control\" href=\"#lb-{{idGallery}}\" role=\"button\" data-slide=\"prev\" data-bs-slide=\"prev\">\n                                <span class=\"mobi-mbri mobi-mbri-arrow-prev\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden\">Previous</span>\n                            </a>\n                            <a class=\"carousel-control-next carousel-control\" href=\"#lb-{{idGallery}}\" role=\"button\" data-slide=\"next\" data-bs-slide=\"next\">\n                                <span class=\"mobi-mbri mobi-mbri-arrow-next\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden\">Next</span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
