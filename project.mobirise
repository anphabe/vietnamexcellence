          "_customHTML": "<section data-bs-version=\"5.1\" class=\"gallery5 mbr-gallery\" group=\"Sliders & Galleries\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\">\n    <mbr-parameters>\n        <input type=\"id\" name=\"idGallery\">\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <select title=\"Columns\" name=\"cardsWidth\">\n            <option value=\"12\">1</option>\n            <option value=\"6\">2</option>\n            <option value=\"4\" selected>3</option>\n            <option value=\"3\">4</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <header>Images</header>\n        <input type=\"checkbox\" title=\"Auto Size\" name=\"autoSize\">\n        <input type=\"range\" name=\"imageHeight\" title=\"Height\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"autoSize && showItemSubtitle\">\n        <input type=\"checkbox\" title=\"Spacing\" name=\"spacing\" checked>\n        <input type=\"checkbox\" title=\"Caption\" name=\"showItemSubtitle\" checked>\n        <header>Popup Slider</header>\n        <input type=\"checkbox\" title=\"Fade Effect\" name=\"fade\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"3\" max=\"10\" value=\"5\" step=\"1\" condition=\"autoplay\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type!=='color'\">Overlay</header>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#fafafa\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth,\n                    'container-fluid': fullWidth}\">\n        \n        <div class=\"row mbr-gallery\">\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image active\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/highlandscoffee.jpg\" alt data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>HIGHLANDS COFFEE</b></h4>\n                      <h6 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-4\" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"></h6>\n                </div>\n            </div>\n\n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/team-suntory.jpeg\" alt data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><b>SUNTORY PEPSICO\n</b><div><b>VIETNAM BEVERAGE</b></div></h4>\n                      <h6 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-4\" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"></h6>\n                </div>\n            </div>\n            \n            <div class=\"col-12 col-md-3 col-lg-{{cardsWidth}} item gallery-image\">\n                <div class=\"item-wrapper\" data-toggle=\"modal\" data-bs-toggle=\"modal\" data-target=\"#{{idGallery}}-modal\" data-bs-target=\"#{{idGallery}}-modal\">\n                    <img class=\"w-100\" src=\"@PROJECT_PATH@/assets/images/marico-sea.jpeg\" alt=\"ÔNG. VAIBHAV BHANCHAWAT\" data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"3\" data-bs-slide-to=\"3\">\n                    <div class=\"icon-wrapper\">\n                        <span class=\"mobi-mbri mobi-mbri-search mbr-iconfont mbr-iconfont-btn\"></span>\n                    </div>\n                </div>\n                <div class=\"winner-caption\">\n                      <h4 class=\"mbr-item-subtitle mbr-fonts-style align-center  mt-4 \" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\">ÔNG. <b>VAIBHAV BHANCHAWAT</b></h4>\n                      <h6 class=\"mbr-item-subtitle mbr-fonts-style align-center mb-4\" mbr-theme-style=\"display-7\" mbr-if=\"showItemSubtitle\"><i>Tổng Giám Đốc - Marico\n</i><div><i>Đông Nam Á và Nam Phi</i></div></h6>\n                </div>\n            </div>\n            \n        </div>\n\n        <div class=\"modal mbr-slider\" id=\"{{idGallery}}-modal\" tabindex=\"-1\" role=\"dialog\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\" role=\"document\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-body\">\n                        <div id=\"lb-{{idGallery}}\" class=\"carousel slide\" mbr-class=\"{'carousel-fade': fade}\" data-ride=\"{{autoplay ? 'carousel' : false}}\" data-bs-ride=\"{{autoplay ? 'carousel' : false}}\" data-interval=\"{{interval*1000}}\" data-bs-interval=\"{{interval*1000}}\">\n                            <div class=\"carousel-inner\">\n                                <div class=\"carousel-item active\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">LÃNH ĐẠO TRUYỀN CẢM HỨNG</h2>\n                                            \n                                        </div>\n                                        <div class=\"col-12 col-md-4 pb-5\">\n                                          <picture>\n                                            <source media=\"(min-width:10px)\" srcset=\"assets/images/highlandscoffee.jpg\">\n                                            <img src alt style>\n                                          </picture>\n\n                                            \n                                        </div>\n                                        <div class=\"col-12 col-md-8 pb-5\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                                  Hành trình của Highlands Coffee bắt đầu từ tầm nhìn và khát khao “mang những điều tốt nhất của Việt Nam ra thế giới” của người sáng lập - Ông David Thai. Khát khao ấy cũng chính là giấc mơ, nguồn cảm hứng chung cho toàn bộ Ban Lãnh Đạo và đội ngũ nhân viên gắn kết với mục tiêu xây dựng và phát triển vững mạnh. Trong 23 năm qua, dưới sự dẫn dắt xuất sắc của đội ngũ lãnh đạo tâm huyết, Highlands Coffee đã có những thành công vượt trội với hơn 600 cửa hàng tại Việt Nam và từng bước vươn tầm thế giới với hơn 50 cửa hàng tại Philippines.\n                                                </p>\n                                            </div>\n                                        </div>\n                                        <div class=\"col-1\">\n                                          <picture>\n                                            <source media=\"(min-width:10px)\" srcset=\"assets/images/icon-quote.png\">\n                                            <img src alt style>\n                                          </picture>\n                                        </div>\n\n                                        <div class=\"col-10 col-md-6\">\n                                            <p class=\"mbr-text mbr-fonts-style text-start quote-content\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                              Đây là hồ sơ rất truyền cảm hứng, mô tả cuộc hành trình và tác động của lãnh đạo David Thai. Được dẫn dắt bởi tầm nhìn và phong cách lãnh đạo xuất sắc từ người sáng lập David Thái, Doanh nghiệp đã kết hợp giữa chuyên môn địa phương cùng các tiêu chuẩn quốc tế tốt nhất, hứa hẹn đưa Highlands trở thành một thương hiệu toàn cầu, giới thiệu với thế giới nét tinh tế, văn hóa và các sản phẩm chất lượng của Việt Nam.\n                                            </p>\n\n                                        </div>\n                                        <div class=\"col-12 col-md-4 quote-author\">\n                                          \n                                            <div>\n                                              <picture>\n                                                <source media=\"(min-width:10px)\" srcset=\"assets/images/joshua-thumb-2023.jpg\">\n                                                <img src alt style>\n                                              </picture>\n                                            </div>\n                                            <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                              Mr. <b>JOSHUA JAMES</b><br>\n                                              Board Member of The British Chamber Of Commerce Vietnam (BritCham);<br>\n                                              Vice Chairman – HR &amp; Training Sector Committee, European Chamber of Commerce in Vietnam (EuroCham)\n                                            </p>\n                                        </div>\n                                    </div>\n                                </div>\n                                                                \n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">LÃNH ĐẠO TRUYỀN CẢM HỨNG</h2>\n                                            \n                                        </div>\n                                        <div class=\"col-12 col-md-4 pb-5\">\n                                          <picture>\n                                            <source media=\"(min-width:10px)\" srcset=\"assets/images/team-suntory.jpeg\">\n                                            <img src alt style>\n                                          </picture>\n                                        </div>\n                                        <div class=\"col-12 col-md-8 pb-5\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                                  Với tầm nhìn hướng về tương lai cùng tinh thần chấp nhận thử thách – phá vỡ giới hạn, đội ngũ lãnh đạo của SPVB đã dẫn dắt doanh nghiệp trở thành tập đoàn nước giải khát trị giá hơn 1 tỷ USD. Bí quyết của họ không chỉ là tinh thần tiên phong và cam kết bền vững, mà còn là việc hướng tới tương lai bằng sự sáng tạo không ngừng, tối ưu hóa quy trình và đầu tư vào chuyển đổi số và chăm sóc anh sinh cho nhân viên.  \n                                                </p>\n                                            </div>\n                                        </div>\n                                        <div class=\"col-1\">\n                                          <picture>\n                                            <source media=\"(min-width:10px)\" srcset=\"assets/images/icon-quote.png\">\n                                            <img src alt style>\n                                          </picture>\n                                        </div>\n\n                                        <div class=\"col-10 col-md-6\">\n                                            <p class=\"mbr-text mbr-fonts-style text-start quote-content\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                              Suntory Pepsico tiếp tục dẫn đầu không chỉ trong ngành hàng của họ mà còn là một trong các doanh nghiệp dẫn đầu ở thị trường Việt Nam, với họ kết quả kinh doanh gắn liền với sự phát triển của con người để đảm bảo tính bền vững. Đó không chỉ là một tầm nhìn mà còn là khả năng \"Live the Vision\" (tạm dịch: Sống cùng tầm nhìn) khiến họ khác biệt với những doanh nghiệp khác. Kết quả kinh doanh là minh chứng thực tế cho tầm nhìn mà họ đặt ra, tiếp tục chứng minh tương lai bền vững của doanh nghiệp chính là đầu tư vào thế hệ kế thừa tiếp theo từ quy trình đến con người đến cải tiến sản phẩm.\n                                            </p>\n\n                                        </div>\n                                        <div class=\"col-12 col-md-4 quote-author\">\n                                          \n                                            <div>\n                                              <picture>\n                                                <source media=\"(min-width:10px)\" srcset=\"assets/images/winnie-thumb-2023.jpg\">\n                                                <img src alt style>\n                                              </picture>\n                                            </div>\n                                            <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                             Ms. <b>WINNIE LAM</b><br>\n                                              General Secretary to the Hong Kong Business Association Vietnam (HKBAV);<br>\n                                              Board Member of The Canadian Chambers of Commerce in Vietnam (CanCham)<br>\n                                            </p>\n                                        </div>\n                                    </div>\n                                </div>\n                                \n                                <div class=\"carousel-item\">\n                                    <div class=\"row\">\n                                        <div class=\"col-12 winner-intro\">\n                                            <h2 class=\"title-category mbr-section-subtitle mbr-fonts-style mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">LÃNH ĐẠO TRUYỀN CẢM HỨNG</h2>\n                                            \n                                        </div>\n                                        <div class=\"col-12 col-md-4 pb-5\">\n                                         \n                                         <picture>\n                                            <source media=\"(min-width:10px)\" srcset=\"assets/images/marico-sea.jpeg\">\n                                            <img src alt style>\n                                          </picture>\n\n                                            \n                                        </div>\n                                        <div class=\"col-12 col-md-8 pb-5\">\n                                            <div class=\"content-bio\">\n                                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                                  Với kinh nghiệm quản lý hơn 23 năm tại nhiều thị trường và tư duy cấp tiến, Ông. Vaibhav Bhanchawat đã giúp thay đổi bức tranh kinh doanh của Marico Sea, lội ngược dòng ngoạn mục với mức tăng trưởng ấn tượng. Giá trị văn hóa tập trung vào con người cùng chiến lược khác biệt giúp nhân viên định hướng chung một tầm nhìn, được truyền cảm hứng và tập trung sức mạnh hành động dẫn đến những thành công to lớn hơn phía trước.\n                                                </p>\n                                            </div>\n                                        </div>\n                                        <div class=\"col-1\">\n                                          <picture>\n                                            <source media=\"(min-width:10px)\" srcset=\"assets/images/icon-quote.png\">\n                                            <img src alt style>\n                                          </picture>\n                                        </div>\n\n                                        <div class=\"col-10 col-md-6\">\n                                            <p class=\"mbr-text mbr-fonts-style text-start quote-content\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                              Bằng tầm nhìn đầy tham vọng và chiến lược xuất sắc của mình, ông Vaibhav đã thay đổi bức tranh kinh doanh của Marico SEA. Những nỗ lực không ngừng của ông trong việc xây dựng một nền văn hóa mạnh mẽ được nhắc lại trong toàn tổ chức cũng là tấm gương tuyệt vời cho các nhân viên của công ty cùng nhau vượt qua những thách thức kinh doanh để trở thành công ty hàng đầu hiện nay trong lĩnh vực chăm sóc tóc cho nam giới với mức tăng trưởng gấp đôi.\n                                            </p>\n\n                                        </div>\n                                        <div class=\"col-12 col-md-4 quote-author\">\n                                          \n                                            <div>\n                                              <picture>\n                                                <source media=\"(min-width:10px)\" srcset=\"assets/images/khuong-thumb-2023.jpg\">\n                                                <img src alt style>\n                                              </picture>\n                                            </div>\n                                            <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\">\n                                              Professor <b>NGUYEN DUC KHUONG</b><br>\n                                              Member of The Prime Minister's Economic Advisory Group;<br>\n                                              President, Association of Vietnamese Scientists and Experts (AVSE Global)\n\n                                            </p>\n                                        </div>\n                                    </div>\n                                </div>\n\n                                \n                            </div>\n\n                            <ol class=\"carousel-indicators\">\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"active\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\"></li>\n                                <li data-target=\"#lb-{{idGallery}}\" data-bs-target=\"#lb-{{idGallery}}\" data-slide-to=\"2\" data-bs-slide-to=\"2\"></li>\n                                \n                            </ol>\n                            <a role=\"button\" href class=\"close\" data-dismiss=\"modal\" data-bs-dismiss=\"modal\" aria-label=\"Close\">\n                            </a>\n                            <a class=\"carousel-control-prev carousel-control\" href=\"#lb-{{idGallery}}\" role=\"button\" data-slide=\"prev\" data-bs-slide=\"prev\">\n                                <span class=\"mobi-mbri mobi-mbri-arrow-prev\" aria-hidden=\"true\"></span>\n                                <span class=\"sr-only visually-hidden\">Previous</span>\n                            </a>\n                            <a class=\"carousel-control-next carousel-control\" href=\"#lb-{{idGallery}}\" role=\"button\" data-slide=\"next\" data-bs-slide=\"next\">\n                                <span class=\"mobi-mbri mobi-mb